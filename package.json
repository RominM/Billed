{
  "scripts": {
    "test": "jest --coverage --noStackTrace --silent"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./setup-jest.js",
      "jest-localstorage-mock"
    ],
    "globals": {
      "window": {}
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/app/**",
